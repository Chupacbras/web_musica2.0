los/fuentes.css'\nimport Toolbar from './Toolbar';\nimport Home from './Home';\nimport About from './About';\nimport Service from './Service';\nimport Media from './Media';\nimport Contact from './Contact';\nimport MusicPlayer from './MusicPlayer';\nimport React, { useRef, useEffect } from 'react';\n\nfunction App() {\n\n  const ref = {\n    homeRef: useRef(null),\n    aboutMeRef: useRef(null),\n    serviceRef: useRef(null),\n    mediaRef: useRef(null),\n    contactRef: useRef(null),\n  }\n\n  const audio = [\n    \"/audio/2013 - Nits d'Estiu - El Secret De París.mp3\",\n    \"/audio/2016 - Guerra - Marcos Franz.mp3\",\n    \"/audio/2015 - No Tenemos Cura - Zarigüeya.mp3\",\n    \"/audio/2016 - No Et Vull Deixar Marxar - Marcos Franz.mp3\",\n    \"/audio/2014 - Come On Kill Me - Full equip.mp3\",\n    \"/audio/2013 - Mira't - El Secret De París.mp3\",\n    \"/audio/2015 - Salta - Zarigüeya.mp3\",\n\n  ]\n\n  return (\n    <div>\n      <MusicPlayer audioFiles={audio} />\n      <div>\n        <Toolbar references={ref} />\n      </div>\n      <div ref={ref.homeRef}>\n        <Home />\n      </div>\n      <div ref={ref.aboutMeRef}>\n        <About />\n      </div>\n      <div ref={ref.serviceRef}>\n        <Service />\n      </div>\n      <div ref={ref.mediaRef}>\n        <Media />\n      </div>\n      <div ref={ref.contactRef}>\n        <Contact />\n      </div>\n    </div>\n\n  );\n}\n\nexport default App;\n","/Users/albertformatger/Documents/GitHub/web_musica2.0/src/reportWebVitals.js",[],[],"/Users/albertformatger/Documents/GitHub/web_musica2.0/src/Service.js",["68","69","70"],[],"import './Estilos/Service.css';\n\nfunction Service() {\n    return (\n        <div className=\"Service-container\">\n            <h className='Title-service'>SERVICIOS</h>\n            <div className='container-card'>\n                <div className='card' >\n                    <div className='card-image'>\n                        <img src='/fotos/49.JPG' />\n                    </div>\n                    <div className='card-text'>\n                        <h className='title-card'>\n                            Eventos musicales\n                        </h>\n                        <p className='into-card'>\n                            <ul>\n                                <li>Conciertos</li><br />\n                                <li>Sustituciones</li><br />\n                                <li>Alquiler de back-line</li><br />\n                                <li>Producción y road management</li><br />\n                            </ul>\n                        </p>\n                    </div>\n                </div>\n                <div className='card' >\n                    <div className='card-image' >\n                        <img src='/fotos/servicios_clases.png' />\n                    </div>\n                    <div className='card-text'>\n                        <h className='title-card'>\n                            Clases de guitarra\n                        </h>\n                        <p className='into-card'>\n                            <ul>\n                                <li>Modalidad online / presencial</li><br />\n                                <li>Todos los estilos: Blues, Rock, Heavy, Funk, Jazz, Pop</li><br />\n                                <li>Teoría musical, harmonía y solfeo</li><br />\n                                <li>Composición, grabación y edición musical</li><br />\n                            </ul>\n                        </p>\n                    </div>\n                </div>\n                <div className='card' >\n                    <div className='card-image'>\n                        <img src='/fotos/25.JPG' />\n                    </div>\n                    <div className='card-text'>\n                        <h className='title-card'>\n                            Músico de sesión\n                        </h>\n                        <p className='into-card'>\n                            <ul>\n                                <li>Modalidad online / presencial</li><br />\n                                <li>Grabación de guitarras</li><br />\n                                <li>Composición de arreglos para canciones</li><br />\n                                <li>Composición musical</li><br />\n                                <li>Producción musical</li><br />\n                            </ul>\n                        </p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    );\n}\nexport default Service;","/Users/albertformatger/Documents/GitHub/web_musica2.0/src/About.js",["71","72","73","74","75"],[],"import React, { useState } from \"react\";\nimport './Estilos/About.css';\n\nfunction About() {\n\n    const abrirCorreo = () => {\n        window.location.href = \"mailto:albert_furma@hotmail.com\";\n    };\n\n    const abrirInstagram = () => {\n        window.open(\"https://www.instagram.com/albertformatger/\", \"_blank\");\n    };\n\n    const abrirYouTube = () => {\n        window.open(\"https://www.youtube.com/@AlbertFormatger\", \"_blank\");\n    };\n\n    return (\n        <div className=\"About\">\n            <div className=\"Descripcion\">\n                <h className='Title'>SOBRE MÍ</h>\n                <div className=\"contenedor-contenido\">\n                    <a className=\"contenido\">\n                        Nacido el 21/02/1995 en Barcelona, España.\n                        Se inició en el mundo de la música a los 12 años cuando le regalaron a su hermana una guitarra eléctrica.\n                        Es el primer músico de la familia.\n                        <br /><br />\n                        Al poco tiempo, su hermana empezó a prestarle la guitarra para que iniciara su aprendizaje por cuenta propia. Fue en aquel entonces cuando sus padres compartieron con él sus gustos musicales.\n                        <br /><br />\n                        Aprendió tocando piezas musicales de las grandes leyendas de las seis cuerdas como Angus Young, Ritchie Blackmore, Slash, BB King, David Gilmourn Clapton, Hendrix…\n                        <br /><br />\n                        A los 2 años de chapurrear y aprender lo que cazaba por youtube, al ver la facilidad en el instrumento, recibió clases de guitarra 30’ semanales durante 4 años con grandes docentes. Luego continuó formandose autodidactamente como al inicio.\n                        A los 14 años ya había formado su primera banda de rock con la que actualmente sigue en funcionamiento.\n                        <br /><br />\n                        A los 16 años grabó un EP de 4 temas con un grupo de pop-rock catalán con el que entró en formación y otro con su banda de rock.\n                        <br /><br />\n                        A esa edad recibió por parte de su padre, una guitarra construida por el 100% artesanal, más adelante llegó otra y actualmente la tercera está en proceso.\n                        <br /><br />\n                        A los 17 años participó en la grabación y composición de arreglos de guitarra del álbum del artista Marcos Franz y a partir de ese momento\n                        han sido varias las colaboraciones con diferentes artistas.\n                        <br /><br />\n                        Desde los 18 años ha dedicado gran parte de su tiempo al mundo del directo, dónde la demanda ha ido en augmento hasta dia de hoy.\n                        <br /><br />\n                        Actualmente se encuentra en pleno proceso de grabación de su primer disco con la banda de rock Black Amber,\n                        se dedica al mundo del directo (destacando a Fitos y el Fitipaldi - tributo), a la enseñanza del instrumento y sesiones de estudio\n                        en modalidad online y presencial. Ah!! Todo esto compajinado con un trabajo de ingenieria.\n\n\n                        <div className=\"contenedor-icon\">\n                            <img src='/icon/instagram.png' onClick={abrirInstagram} />\n                            <img src='/icon/youtube.png' onClick={abrirYouTube} />\n                        </div>\n                    </a>\n                </div>\n            </div>\n            <div className=\"Foto_desc\">\n            </div>\n        </div>\n    );\n}\n\nexport default About;\n","/Users/albertformatger/Documents/GitHub/web_musica2.0/src/Home.js",[],[],"/Users/albertformatger/Documents/GitHub/web_musica2.0/src/Media.js",[],[],"/Users/albertformatger/Documents/GitHub/web_musica2.0/src/Contact.js",[],[],"/Users/albertformatger/Documents/GitHub/web_musica2.0/src/MusicPlayer.js",[],[],"/Users/albertformatger/Documents/GitHub/web_musica2.0/src/Toolbar.js",["76","77","78","79","80","81","82","83","84","85","86"],[],"import React, { useEffect, useState, useRef } from 'react';\nimport './Estilos/Toolbar.css';\n\nconst Toolbar = (props) => {\n\n    const [isScrolldown, setIsScrolldown] = useState(false);\n    const [isAtTop, setIsAtTop] = useState(false);\n    const [isPause, setIsPause] = useState(false);\n    const [isMenuOpen, setIsMenuOpen] = useState(false);\n\n    useEffect(() => {\n        let timeout;\n        let lastScrollTop = 0;\n        const handleScroll = () => {\n\n            if (window.scrollY === 0) {\n                if (timeout) {\n                    clearTimeout(timeout);\n                }\n\n                timeout = setTimeout(() => {\n                    setIsAtTop(true);\n                    setIsScrolldown(false);\n                }, 750);\n            }\n\n            else {\n                if (window.scrollY > lastScrollTop) {\n                    setIsAtTop(false);\n                    setIsScrolldown(true);\n                    if (timeout) {\n                        clearTimeout(timeout);\n                    }\n\n                    timeout = setTimeout(() => {\n                        setIsPause(true);\n                    }, 750);\n                }\n                else {\n                    setIsAtTop(false);\n                    setIsPause(false);\n                    if (timeout) {\n                        clearTimeout(timeout);\n                    }\n                    timeout = setTimeout(() => {\n                        setIsPause(true);\n                    }, 750);\n                }\n            }\n\n            lastScrollTop = window.scrollY;\n        };\n\n        window.addEventListener('scroll', handleScroll);\n\n        return () => {\n            window.removeEventListener('scroll', handleScroll);\n        };\n    },\n        []\n    );\n\n    const scrollToSection = (ref) => {\n        if (ref.current) {\n            ref.current.scrollIntoView({ behavior: 'smooth' });\n        }\n    };\n\n    const { references } = props;\n\n    return (\n        <div className={`Toolbar ${isScrolldown ? 'scroll-down' : ''} ${isAtTop ? 'at-top' : ''} ${isPause ? 'is-pause' : ''}`} >\n            <div className='Toolbar-nombre'>\n                <h1 className={`Toolbar-nombre2 ${isPause ? 'is-pause' : ''}`}>\n                    Albert Formatger\n                </h1>\n            </div>\n            <div className={`HamburguesaButton ${isScrolldown ? 'scroll-down' : ''} ${isAtTop ? 'at-top' : ''} ${isPause ? 'is-pause' : ''}`} onClick={() => setIsMenuOpen(!isMenuOpen)}>\n                ☰\n            </div>\n            {isMenuOpen && (\n                <div className='Mobile-menu'>\n                    <a className='Mobile-menu-item' onClick={() => { scrollToSection(references.homeRef); setIsMenuOpen(false) }} >Home</a>\n                    <a className='Mobile-menu-item' onClick={() => { scrollToSection(references.aboutMeRef); setIsMenuOpen(false) }}>Sobre mí</a>\n                    <a className='Mobile-menu-item' onClick={() => { scrollToSection(references.serviceRef); setIsMenuOpen(false) }}>Servicios</a>\n                    <a className='Mobile-menu-item' onClick={() => { scrollToSection(references.mediaRef); setIsMenuOpen(false) }}>Media</a>\n                    <a className='Mobile-menu-item' onClick={() => { scrollToSection(references.contactRef); setIsMenuOpen(false) }}>Contacto</a>\n                </div>\n            )}\n\n            <div className='Toolbar-button'>\n                <a className={`Button-style ${isPause ? 'is-pause' : ''}`} onClick={() => scrollToSection(references.homeRef)}>Home</a>\n                <a className={`Button-style ${isPause ? 'is-pause' : ''}`} onClick={() => scrollToSection(references.aboutMeRef)}>Sobre mí</a>\n                <a className={`Button-style ${isPause ? 'is-pause' : ''}`} onClick={() => scrollToSection(references.serviceRef)}>Servicios</a>\n                <a className={`Button-style ${isPause ? 'is-pause' : ''}`} onClick={() => scrollToSection(references.mediaRef)}>Media</a>\n                <a className={`Button-style ${isPause ? 'is-pause' : ''}`} onClick={() => scrollToSection(references.contactRef)}>Contacto</a>\n            </div>\n        </div>\n    )\n}\n\nexport default Toolbar;","/Users/albertformatger/Documents/GitHub/web_musica2.0/src/Image_slider.js",["87"],[],"import React from \"react\";\nimport ReactDOM from \"react-dom\";\nimport Flickity from \"react-flickity-component\";\nimport './Estilos/Media.css';\n\nfunction Carousel({ images}) {\n\n    const flickityOptions = {\n        initialIndex: 3,\n        wrapAround: true,\n        selectedAttraction: 0.01,\n        autoPlay:true,\n        prevNextButtons: true,\n        pageDots: false,\n        pauseAutoPlayOnHover: false,\n        arrowShape: {\n            x0: 30,\n            x1: 65, y1: 35,\n            x2: 65, y2: 35,\n            x3: 55\n        }\n\n\n    }\n    return (\n        <div className=\"fotos-container\">\n            <h1 className='Title-media'>LIVE MEDIA</h1>\n            <div className=\"carousel-container\">\n                <Flickity\n                className={'fotos'}\n                options={flickityOptions}\n                >\n                 